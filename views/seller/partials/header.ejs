<body>
    <% if (seller != ''){ %>
    <div id="navs" class="navs fixed-top">
        <div class="container">
            <nav class="navbar all navbar-expand-lg">
                <a class="navbar-brand animate__animated animate__zoomInLeft" href="/seller"><i
                        class="fab fa-shopify" style="font-size: 30px;"></i></a>
                <a class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </a>
                <div class="collapse navbar-collapse align-items-center text-start" id="navbarSupportedContent">
                    <ul
                        class="navbar-nav me-auto mb-2 mb-lg-0 justify-content-center flex-fill animate__animated animate__zoomIn ">
                        <li class="nav-item"> 
                            <a class="nav-link active" aria-current="page" href="/seller">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/seller/upload">Upload</a>
